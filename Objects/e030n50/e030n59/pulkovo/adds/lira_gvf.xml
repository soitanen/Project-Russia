<?xml version="1.0"?>

<PropertyList>

 <path>lira_gvf.ac</path>

 <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
 </animation>


 <animation>
  <type>rotate</type>
  <object-name>Armature</object-name>
  <property>/sim/time/elapsed-sec</property>
  <factor>-90.0</factor>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

</PropertyList>
